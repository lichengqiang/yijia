.kongview {
   position: fixed;
   top: 0;
   left: 0;
   height: 100vh;
   width: 100vw;
   z-index: 1;
}

.button_no {
   opacity: 0;
}

.formclass {
   width: 100%;
   height: 100%;
   position: absolute;
   top: 0;
   left: 0;
   opacity: 0;
   z-index: 8;
}

.copy {
   position: absolute;
   left: 80rpx;
   bottom: -20rpx;
   z-index: 101;
   padding: 10rpx 20rpx;
   color: #fff;
   font-size: 28rpx;
   background-color: #000;
   white-space: nowrap;
}

.copy::after {
   content: '';
   width: 20rpx;
   height: 20rpx;
   background-color: #000;
   transform: rotate(45deg);
   position: absolute;
   top: -8rpx;
   left: 42%;
}

text {
