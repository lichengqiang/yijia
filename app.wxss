/* 微信弹窗 */

.weixin_tc {
   width: 100%;
   position: fixed;
   /* top:-100vh; */
   left: 0;
   padding: 0 30rpx;
   background-color: #fff;
   z-index: 1000;
   /* border-bottom:4rpx solid #6b76ff; */
   box-shadow: 0 20rpx 60rpx rgba(0, 0, 0, 0.2);
   overflow: hidden;
   opacity: 0;
   pointer-events: none;
   visibility: hidden;
   height: 100vh;
}

.weixin_tc_xs {
   opacity: 1;
   pointer-events: auto;
   visibility: visible;
   top: 0;
}

.weixin_tc_img {
   margin-top: 30rpx;
   padding-bottom: 30rpx;
   border-bottom: 2rpx solid #eee;
}

.weixin_tc_img>image {
   width: 300rpx;
   margin-right: 20rpx;
}

.weixin_tc_img>view {
   font-size: 28rpx;
}

.weixin_tc_text>view:nth-of-type(1) {
   font-size: 42rpx;
   font-weight: bold;
   color: #6b76ff;
   margin: 30rpx 0;
}

.weixin_tc_text>view:nth-last-of-type(1) {
   width: 500rpx;
   padding: 10rpx;
   background-color: #f1f1f1;
   border: 2rpx solid #e1e1e1;
   text-align: center;
   border-radius: 10rpx;
   margin: 0 auto;
}

.weixin_tc_gb {
   width: 60rpx;
   margin: 0 auto;
