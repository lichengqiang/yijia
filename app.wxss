.home {
   padding: 0 30rpx;
}

.pr {
   position: relative;
}

.tac {
   text-align: center;
}

.container {
   height: 100%;
   display: flex;
   flex-direction: column;
   align-items: center;
   justify-content: space-between;
   padding: 200rpx 0;
   box-sizing: border-box;
}

.roll {
   width: 100%;
   height: 100vh;
   overflow: hidden;
}

view, textarea {
   box-sizing: border-box;
}

.absolute_center {
   transform: translate(-50%, -50%);
   -webkit-transform: translate(-50%, -50%);
   -moz-transform: translate(-50%, -50%);
   -o-transform: translate(-50%, -50%);
}

.lbli_tz {
   position: absolute;
   top: 0;
   left: 0;
   width: 100%;
   height: 100%;
   z-index: 100;
}

.lbli_tzxx {
   position: absolute;
   top: 0;
   left: 0;
   width: 150%;
   height: 150%;
   z-index: 100;
}

.restricted_single {
   white-space: nowrap;
   text-overflow: ellipsis;
   overflow: hidden;
}

.comment_inner {
   display: -webkit-box;
   -webkit-box-orient: vertical;
   -webkit-line-clamp: 2;
   overflow: hidden;
}

.ty_khh {
   -ms-flex-wrap: wrap;
   flex-wrap: wrap;
}

.ty_xsx {
   flex-shrink: 0;
}

