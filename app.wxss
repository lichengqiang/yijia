/* 搜索 */

.search_icon {
   width: 40rpx;
   height: 40rpx;
   border: 2rpx solid #999;
   border-radius: 50%;
   position: relative;
   margin-right: 20rpx;
}

.search_icon::after {
   width: 2rpx;
   height: 8rpx;
   background-color: #999;
   content: '';
   position: absolute;
   bottom: 0;
   right: 0;
   transform: rotate(-45deg);
}

.search_text {
   font-size: 24rpx;
   color: #999;
}

.search_main {
   height: 80rpx;
   border-radius: 100rpx;
   padding: 0 30rpx;
   background-color: #f7f7f7;
}

.new_project {
   margin: 0;
   /* margin-right: 20rpx; */
}

.new_project_type {
   left: 30rpx;
   margin: 0 auto;
}

#share {
   margin-left: 20rpx;
}

/* 上传文件 */

.uploading_file {
   font-size: 28rpx;
   color: #0391de;
   width: 100%;
   margin-bottom: 10rpx;
   padding: 20rpx;
   background-color: #f7f7f7;
}

.uploading_file>image {
   width: 40rpx;
   margin-right: 10rpx;
}

.delete_image_file {
   max-height: 100%;
}

.uploading_select {
   position: fixed;
   bottom: 0;
   left: 0;
   z-index: 1000000;
   width: 100%;
   background-color: #fff;
   height: 260rpx;
   border-top: 1px solid #eee;
}

.uploading_select>view {
   margin: 0 60rpx;
   font-size: 28rpx;
   color: #666;
   text-align: center;
}

.uploading_select_img {
   width: 120rpx;
   height: 120rpx;
   border-radius: 50%;
   background-color: rgba(3, 145, 222, 1);
   margin-bottom: 20rpx;
}

.uploading_select_img>image {
   width: 50rpx;
}

.uploading_select_img_lv {
   background-color: #57bf37;
}

.uploading_select_shade {
   position: fixed;
   width: 100%;
   height: calc(100vh - 260rpx);
   top: 0;
   left: 0;
   margin: 0 !important;
}

.loading_box {
   position: fixed;
   top: calc(45% - 70rpx);
   left: calc(50% - 70rpx);
   width: 140rpx;
   height: 140rpx;
   z-index: 99999999999;
   border-radius: 20rpx;
   background-color: rgba(0, 0, 0, 0.4);
}

.loading_img {
   width: 72rpx;
   height: 72rpx;
   animation: haha1 1s linear infinite;
}

@keyframes haha1 {
   0% {
      transform: rotate(0deg);
   }

   50% {
      transform: rotate(180deg);
   }

   100% {
      transform: rotate(360deg);
   }
}

.lib-pr {
   position: relative;
}

/* 遮罩 */

.shade_502 {
   position: fixed;
   top: 0;
   left: 0;
   background-color: #fff;
   width: 100%;
   height: 100vh;
   z-index: 99999;
   padding-bottom: 140rpx;
}

.shade_502_conceal {
   opacity: 0;
   visibility: hidden;
   pointer-events: none;
}

.shade_502_show {
   opacity: 1;
}

.shade_502 image {
   width: 600rpx;
}

.shade_502_text {
   font-size: 32rpx;
   color: #999;
   margin-top: 20rpx;
}

.shade_502_button {
   width: 320rpx;
   padding: 30rpx 0;
   font-size: 32rpx;
   color: #fff;
   background-color: #833cc0;
   border-radius: 100rpx;
   margin: 40rpx auto 0;
}

