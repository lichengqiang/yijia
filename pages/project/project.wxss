  position: relative;
}

.project_tab>view {
  width: 190rpx;
  height: 85rpx;
  border-radius: 50%;
  margin-left: 20rpx;
  padding: 0;
}

.project_tab>.background_tab {
  width: 190rpx;
  height: 85rpx;
  border-radius: 100rpx;
  position: absolute;
  margin: 0;
}

.bt_animation {
  top: 30rpx;
  right: 210rpx;
  background-color: #e9e93e;
}

.bt_animation1 {
  top: 30rpx;
  right: 0rpx;
}

.pt_icon {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  position: relative;
}

.conduct {
  position: absolute;
  width: 2rpx;
  height: 15rpx;
  top: 7.5rpx;
  left: calc(50% - 1rpx);
}

.conduct1 {
  position: absolute;
  width: 2rpx;
  height: 10rpx;
  bottom: 11rpx;
  left: calc(50% - 1rpx);
  transform: rotate(-45deg);
  transform-origin: center top;
}

.right_icon1 {
  width: 100%;
  height: 100%;
  position: absolute;
  transform: rotate(-45deg) translate(0%, -10%);
  top: -40%;
  left: 0;
}

.complete {
  position: absolute;
  width: 50%;
  height: 2rpx;
  bottom: 0;
  left: 0;
}

.complete1 {
  position: absolute;
  height: 25%;
  width: 2rpx;
  bottom: 0;
  left: 0;
}

