  border-bottom: 2rpx solid #f1f1f1;
  position: relative;
}

.project_list>view:nth-last-child(1) {
  border-bottom: 0;
}

.left_spot {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
