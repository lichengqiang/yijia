}

.regular_callout {
  width: 75rpx;
  min-height: 75rpx;
  border-radius: 100rpx;
  margin-left: 30rpx;
  flex-shrink: 0;
  position: relative;
  overflow: hidden;
  z-index: 101;
}

.one_content {
  align-items: center;
  font-size: 32rpx;
  line-height: 1.6;
  min-height: 75rpx;
  word-break:break-all;
}

.right_dh {
  width: 75rpx;
  height: 75rpx;
  position: relative;
}

.right_icon2 {
  width: 75rpx;
  height: 75rpx;
  position: absolute;
  transform: rotate(-45deg);
  top: -50%;
  left: -5rpx;
}

.rc_ani {
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.rc_animation {
  animation: excitement 10s infiniteinfinite;
}

@keyframes excitement {
  5% {
    transform: scale(1.6);
  }

  10% {
    transform: scale(1);
  }
}

