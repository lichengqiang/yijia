.peo_prohibit::after {
  content: "";
  position: absolute;
  top: 0;
  left: 50%;
  background-color: #e1e1e1;
  width: 2rpx;
  height: 100%;
  transform: translate(-50%, 0%);
}

.peo_prohibit::before {
  content: "";
  position: absolute;
  top: 50%;
  left: 0;
  background-color: #e1e1e1;
  width: 100%;
  height: 2rpx;
  transform: translate(0%, -50%);
}

/*垃圾箱  */

.trash {
  position: absolute;
  top: 0;
  left: 0;
  width: 55rpx;
}

.trash>view {
  width: 100%;
  height: 100%;
  align-items: center;
  position: relative;
}

.trash_top {
  width: 30rpx;
  height: 10rpx;
  border: 2rpx solid #fff;
  border-bottom: 0;
  border-radius: 10rpx 10rpx 0 0;
}

.trash_center {
  width: 100%;
  border-bottom: 2rpx solid #fff;
}

.trash_bottom {
  width: 70%;
  height: calc(100% - 12rpx);
  border: 2rpx solid #fff;
  border-top: 0;
  border-radius: 0 0 10rpx 10rpx;
  position: relative;
}

.trash_bottom1 {
  width: 100%;
  height: 10rpx;
  background-color: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
  opacity: 0.3;
}

/*选中的@  */

.choice_icon {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background-color: rgba(87, 191, 55, 0.9);
  border-radius: 50%;
  display: block;
}

.dn {
  display: none;
}

.ait_position {
  position: absolute;
  bottom: calc(100vh - 406rpx);
  /* bottom: calc(100vh - 550rpx); */
  left: 30rpx;
  align-items: flex-end;
  z-index: 1;
}

.ait_position>view {
  margin-left: 20rpx;
}

.ait_position>view:nth-of-type(1) {
  margin-left: 0;
}

.ap_background {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  color: #fff;
}

/*教程  */

.course_main {
  position: fixed;
  width: 100%;
  height: 100%;
  padding: 0 60rpx;
  left: 0;
  z-index: 1000;
  background-color: rgba(255, 255, 255, 0.9);
}

.about_bottom {
  width: 100%;
  height: 990rpx;
  border-radius: 40rpx;
  box-shadow: 0 0 90rpx rgba(0, 0, 0, 0.2);
  margin: 20rpx 0 60rpx;
  overflow: hidden;
  align-items: center;
  justify-content: center;
  position: relative;
}

.button_position {
  position: absolute;
  bottom: 75rpx;
  left: 50%;
  transform: translate(-50%, 0%);
}

.see_button {
  width: 130rpx;
  height: 130rpx;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  overflow: hidden;
}

.see_button>view {
  height: 55rpx;
  position: relative;
}

.circular_icon {
  width: 55rpx;
  height: 100%;
  border-radius: 50%;
  border: 2rpx solid #fff;
}

.course_li {
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0rpx;
  left: 0;
}

.cl_main {
  padding: 60rpx 60rpx 0;
  width: 100%;
}

.donghua {
  transition: all 0.4s linear 0s;
}

.course_button {
  position: fixed;
  bottom: 20rpx;
  left: 30rpx;
  z-index: 102;
  overflow: hidden;
}

.cb_question {
  width: 60rpx;
  height: 60rpx;
  background-color: rgba(0, 0, 0, 0.2);
  border-radius: 50%;
  font-size: 24rpx;
  color: #fff;
}

.cb_hide {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
}

.pa_introduce {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  transition-delay: 0.5s;
}

.pai_animation {
  opacity: 0;
  top: 60rpx;
}

/*还原删除  */

.restore_delete {
  position: fixed;
  width: 100%;
  height: 80rpx;
  bottom: 0;
  left: 0;
  background-color: rgba(233, 65, 62, 0.8);
  overflow: hidden;
  align-items: center;
  justify-content: center;
  z-index: 103;
}

/*新弹出  */

.popup_pages {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 102;
  background-color: rgba(255, 255, 255, 0.9);
}

.pp_main {
  width: calc(100% - 120rpx);
  height: 990rpx;
  position: absolute;
  text-align: center;
  border-radius: 40rpx;
  top: 47%;
  left: 50%;
}

.ppm_image {
  width: 100%;
  height: 100%;
  border-radius: 40rpx;
  box-shadow: 0 0 90rpx rgba(0, 0, 0, 0.2);
}

.ppm_button {
  position: absolute;
  left: 50%;
  bottom: 50rpx;
  padding: 30rpx 40rpx;
  border-radius: 100rpx;
  transform: translate(-50%, 0);
}

.pp_ps {
  position: absolute;
  bottom: 35rpx;
  left: 100rpx;
  font-size: 24rpx;
  color: #999;
}

.vanish {
  top: 0;
  opacity: 1;
}

.vanishs {
  top: 60rpx;
  opacity: 0;
  visibility: hidden;
  pointer-events: none;
}
